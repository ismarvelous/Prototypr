@{
    ViewBag.Title = "home";
}

<h2>Posts</h2>

<ul>
    @foreach (var post in Site.posts)
    {
        <li>@post.Title</li>
    }
</ul>


<h2>Docs</h2>

<ul>
    @foreach (var doc in Site.docs)
    {
        <li>@(doc.Title) - @(doc.Author)</li>
    }
</ul>

<h2>Permalinks</h2>

<ul>
    <li>Title for url @Site.docs.lorem.Url @Site.docs.lorem.Title</li>
</ul>


